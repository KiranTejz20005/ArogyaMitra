# Copy to .env and fill in. Backend uses backend/.env; frontend uses frontend/.env.local

# Backend (also copy to backend/.env)
SECRET_KEY=your-secret-key
DATABASE_URL=sqlite:///./arogyamitra.db
CORS_ORIGINS=http://localhost:3000
GROQ_API_KEY=

# Frontend (copy to frontend/.env.local). Use NEXT_PUBLIC_API_URL for backend API (required for Render).
NEXT_PUBLIC_API_URL=http://localhost:8000
API_URL=http://localhost:8000
NEXT_PUBLIC_APP_URL=http://localhost:3000
